{
    "name": "shedeza/sybase-ase-orm-bundle",
    "description": "Symfony Bundle for Sybase ASE ORM",
    "version": "1.0.0   ",
    "type": "symfony-bundle",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "symfony/framework-bundle": "^6.0|^7.0",
        "symfony/config": "^6.0|^7.0",
        "symfony/dependency-injection": "^6.0|^7.0",
        "symfony/console": "^6.0|^7.0",
        "ext-pdo": "*",
        "ext-pdo_dblib": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Shedeza\\SybaseAseOrmBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Shedeza\\SybaseAseOrmBundle\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "keywords": [
        "symfony",
        "bundle",
        "sybase",
        "ase",
        "orm",
        "database"
    ],
    "authors": [
        {
            "name": "Shedeza",
            "email": "contact@shedeza.com"
        }
    ],
    "homepage": "https://github.com/shedeza/sybase-ase-orm-bundle",
    "support": {
        "issues": "https://github.com/shedeza/sybase-ase-orm-bundle/issues",
        "source": "https://github.com/shedeza/sybase-ase-orm-bundle"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "6.0|^7.0"
        }
    }
}